<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projectassyifa.githubuserapp">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_github"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_github_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GithubUserApp"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".screen.ProfilActivity"
            android:exported="true" />
        <activity
            android:name=".screen.HomeActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>